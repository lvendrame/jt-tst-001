<!DOCTYPE html>
<html>
  <head>
    <title>Test1</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"/>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <% flash.each do |name, msg| %>
      <% if msg.is_a?(String) %>
        <div class="flash <%= name %>"><%= msg %></div>
      <% end %>
    <% end %>
    <%= yield %>
  </body>
</html>
  <body>
    <%= yield %>
  </body>
</html>
